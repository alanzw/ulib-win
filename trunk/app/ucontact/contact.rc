IDR_MENU_SDP MENU
BEGIN
    POPUP "&File"
    BEGIN
        MENUITEM "&New", IDM_NEW
        MENUITEM "&Exit", IDM_EXIT
    END
    POPUP "&Options"
    BEGIN
        MENUITEM "&Toolbar", IDM_TOOLBAR
        MENUITEM "&Status Bar", IDM_STATUSBAR
        MENUITEM SEPARATOR
        MENUITEM "&Start Simulation", IDM_START
        MENUITEM "&Stop Simulation", IDM_STOP
        MENUITEM "&Set number", IDM_NUMBER
    END
    POPUP "&Help" HELP
    BEGIN
        MENUITEM "&About SDP", IDM_ABOUT
    END
END


