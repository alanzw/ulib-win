//
// Menu resources
//
IDR_MENU_MAIN MENU
BEGIN
    POPUP "&File"
    BEGIN
        MENUITEM "&New Database", IDM_NEW
        MENUITEM "&Load Database...", IDM_LOAD
        MENUITEM SEPARATOR
        MENUITEM "&Save", IDM_SAVE
        MENUITEM "&Save As...", IDM_SAVEAS
        MENUITEM SEPARATOR
        MENUITEM "&Exit", IDM_EXIT
    END
    POPUP "&Edit"
    BEGIN
        MENUITEM "&Cut", IDM_CUT
        MENUITEM "&COPY", IDM_COPY
    END
    POPUP "&Options"
    BEGIN
        MENUITEM "&Toolbar", IDM_TOOLBAR
        MENUITEM "&Status Bar", IDM_STATUSBAR
        MENUITEM SEPARATOR
        MENUITEM "&Start Simulation", IDM_START
        MENUITEM "&Stop Simulation", IDM_STOP
        MENUITEM "&Set number", IDM_NUMBER
    END
    POPUP "&Help" HELP
    BEGIN
        MENUITEM "&About UExplorer", IDM_ABOUT
    END
END


