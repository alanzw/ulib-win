<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="menu" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin\Debug\menu" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj\Debug\" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
					<Add option="-DBUILD_EXE" />
					<Add option="-DHU_DEBUG" />
					<Add directory="..\inc" />
				</Compiler>
				<ResourceCompiler>
					<Add directory="..\res" />
					<Add directory="..\ico" />
				</ResourceCompiler>
				<Linker>
					<Add library="uwin" />
					<Add directory="bin\debug" />
				</Linker>
			</Target>
			<Target title="Release">
				<Option output="bin\Release\menu" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj\Release\" />
				<Option type="0" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
		</Compiler>
		<Linker>
			<Add option="-mwindows" />
			<Add library="gdi32" />
			<Add library="user32" />
			<Add library="kernel32" />
		</Linker>
		<Unit filename="..\demo\menu\menu.rc">
			<Option compilerVar="WINDRES" />
			<Option compiler="gcc" use="1" buildCommand="$rescomp -i $file -J rc -o $resource_output -O coff $res_includes -DMINGW_RC" />
		</Unit>
		<Unit filename="..\demo\menu\resource.h" />
		<Unit filename="..\demo\menu\test_menu.cpp" />
		<Extensions>
			<code_completion />
			<debugger />
			<envvars />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
